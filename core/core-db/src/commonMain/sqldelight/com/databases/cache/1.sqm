-- Это чистый SQL, здесь создаем таблицу в её ПЕРВОНАЧАЛЬНОМ виде
CREATE TABLE IF NOT EXISTS userEntity (
    uid TEXT NOT NULL PRIMARY KEY,
    username TEXT NOT NULL,
    email TEXT,
    name TEXT,
    photoUrl TEXT,
    createdAt INTEGER NOT NULL, -- Убираем AS Instant (в миграциях пишем чистый тип)
    roles TEXT NOT NULL         -- Убираем AS List (в миграциях пишем чистый тип)
);