insertUser:
INSERT OR REPLACE INTO userEntity
VALUES ?;

getUser:
SELECT *
FROM userEntity;

getUserByUid:
SELECT *
FROM userEntity
WHERE uid = :uid;

updateUser:
UPDATE userEntity
SET name = ?,
email = ?,
photoUrl = ?,
bio = ?,
phone = ?,
addresses = ?,
paymentCartNumbers = ?,
currentCity = ?
WHERE uid = ?;

updatePaymentMethod:
UPDATE userEntity
SET paymentCartNumbers = ?
WHERE uid = ?;

updatePhotoUrl:
UPDATE userEntity
SET photoUrl = ?
WHERE uid = ?;